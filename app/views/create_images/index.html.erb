<p id="notice"><%= notice %></p>

<h1>Create Images</h1>

<table>
  <thead>
    <tr>
      <th>File</th>
      <th>View count</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_images.each do |create_image| %>
      <tr>
        <td><%= create_image.file %></td>
        <td><%= create_image.view_count %></td>
        <td><%= create_image.user %></td>
        <td><%= link_to 'Show', create_image %></td>
        <td><%= link_to 'Edit', edit_create_image_path(create_image) %></td>
        <td><%= link_to 'Destroy', create_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create Image', new_create_image_path %>
